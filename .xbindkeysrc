# Increase brightness
"brightnessctl -q -d intel_backlight s +10%"
   XF86MonBrightnessUp

# Decrease brightness
"brightnessctl -q -d intel_backlight s 10%-"
   XF86MonBrightnessDown

"amixer set Master 5%+"
    XF86AudioRaiseVolume

"amixer set Master 5%-"
    XF86AudioLowerVolume

"amixer set Master toggle"
    XF86AudioMute
